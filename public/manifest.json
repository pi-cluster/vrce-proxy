{
  "manifest_version": 3,
  "name": "Proxy Extension",
  "version": "1.0.0",
  "permissions": [
    "proxy",
    "tabs",
    "storage",
    "unlimitedStorage"
  ],
  "host_permissions": ["http://*/*", "https://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "32": "icon32.png"
  }
}
